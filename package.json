{
  "name": "jmespath-vscode",
  "displayName": "jmespath",
  "description": "Query JSON documents using JMESPath",
  "version": "0.0.1",
  "publisher": "jamesls",
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "images/jplogo-stacked.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.evaluateJMESPath"
  ],
  "main": "./out/src/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmespath/jmespath.vscode"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.evaluateJMESPath",
        "title": "JMESPath: Evaluate Expression"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "jmespath": "^0.15.0"
  }
}
